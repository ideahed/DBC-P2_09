<h1>What meal do you want to eat?</h1>
<h2 id="meal_holder"></h2>
<script type="text/javascript">
  var meals = <%= @meals %>;
  
  $(document).ready(function() {
    $('.meal_teller').on('click', function() {
      console.log('yes')
      $('#meal_holder').text(meals[Math.floor(Math.random()*8)]);
    });
  });
</script>


<button class="meal_teller">Tell the meal!</button>
